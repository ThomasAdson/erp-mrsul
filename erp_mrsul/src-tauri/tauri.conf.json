{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ERP MRSUL",
  "version": "0.1.0",
  "identifier": "com.erpmrsul.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },  "app": {
    "windows": [
      {
        "title": "ERP MRSul",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "maximized": false,
        "decorations": false,
        "titleBarStyle": "Overlay",
        "theme": "Dark",
        "backgroundColor": "#1e40af",
        "shadow": true,
        "center": true,
        "transparent": false,
        "alwaysOnTop": false,
        "skipTaskbar": false
      }
    ],    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico",
      "icons/icon.png"
    ]
  }
}
